<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jonathandamon" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>wrapapp</name>
    <description>A minimal wrapper app for server-side mobile development.</description>
    <author email="me@jonathandamon.com" href="http://jonathandamon.com">Jonathan Damon</author>
    <content src="index.html" />
    <icon src="icon.png" />
    <access origin="*" />
    <allow-navigation href="http://pitchpeek.com/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
